#!/usr/bin/env node
'use strict';
/**
 * Simple script to dump the cars config to the current directory.
 * This is needed to support protractor which cannot use asynchronous initialization.
 */

var fs = require('fs');
var carsConfig = require("../lib/index");

carsConfig.init().then(function (config) {
   fs.writeFileSync("cars-config.json", JSON.stringify(config, null, 4));
});